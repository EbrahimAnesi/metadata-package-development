{
  "name": "Package import",
  "testCases": {
    "merge_import": {
      "path": "/api/metadata",
      "method": "POST",
      "description": "Metadata API",
      "bodyFile": "./package.json",
      "query": {
        "mergeMode": "MERGE",
        "strategy": "CREATE_AND_UPDATE"
      },
      "expect": {
        "body": {
          "contains": {
            "status": "OK"
          }
        }
      }
    },
    "replace_import": {
      "path": "/api/metadata",
      "method": "POST",
      "description": "Metadata API",
      "bodyFile": "./package.json",
      "query": {
        "mergeMode": "replace",
        "strategy": "CREATE_AND_UPDATE"
      },
      "expect": {
        "body": {
          "contains": {
            "status": "OK"
          }
        }
      }
    }
  },
  "header": {
    "Content-Type": "application/json"
  }
}